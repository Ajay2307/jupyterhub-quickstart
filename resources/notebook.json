{
    "kind": "List",
    "apiVersion": "v1",
    "items": [
        {
            "kind": "ImageStream",
            "apiVersion": "v1",
            "metadata": {
                "name": "notebook",
                "labels": {
                    "build": "notebook"
                },
                "annotations": {
                    "openshift.io/display-name": "Python Notebook",
                    "description": "Python Notebook application and S2I builder for custom notebooks.",
                    "iconClass": "icon-python",
                    "supports": "python:3.5,python",
                    "tags": "builder,python,jupyter"
                }
            },
            "spec": {
                "lookupPolicy": {
                    "local": true
                },
                "tags": [
                    {
                        "name": "3.5",
                        "annotations": {
                            "openshift.io/display-name": "Python Notebook (Python 3.5)",
                            "description": "Python notebook application and S2I builder for custom notebooks, using Python 3.5.",
                            "iconClass": "icon-python",
                            "sampleRepo": "https://github.com/jakevdp/PythonDataScienceHandbook.git",
                            "supports": "python:3.5,python",
                            "tags": "builder,python,jupyter"
                        }
                    }
                ]
            }
        },
        {
            "kind": "BuildConfig",
            "apiVersion": "v1",
            "metadata": {
                "name": "notebook",
                "labels": {
                    "build": "notebook"
                }
            },
            "spec": {
                "triggers": [
                    {
                        "type": "ConfigChange"
                    },
                    {
                        "type": "ImageChange"
                    }
                ],
                "source": {
                    "type": "Git",
                    "git": {
                        "uri": "https://github.com/getwarped/jupyter-spawner"
                    },
                    "contextDir": "notebook"
                },
                "strategy": {
                    "type": "Source",
                    "sourceStrategy": {
                        "from": {
                            "kind": "DockerImage",
                            "name": "centos/python-35-centos7:latest"
                        }
                    }
                },
                "output": {
                    "to": {
                        "kind": "ImageStreamTag",
                        "name": "notebook:3.5"
                    }
                }
            }
        }
    ]
}
